{
	"name": "base-project-back-end",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"devDependencies": {
		"@techmmunity/eslint-config": "^5.0.4",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.1",
		"@vercel/ncc": "^0.31.1",
		"dotenv": "^10.0.0",
		"eslint": "^8.0.1",
		"husky": "^7.0.2",
		"jest": "^27.3.0",
		"lint-staged": "^11.2.3",
		"prettier": "^2.4.1",
		"ts-jest": "^27.0.7",
		"ts-node-dev": "^1.1.8",
		"tsconfig-paths": "^3.11.0",
		"typescript": "^4.4.4"
	},
	"scripts": {
		"prepare": "husky install",
		"husky:pre-commit": "tsc && list-staged",
		"dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
		"test": "jest",
		"test:cov": "jest --coverage",
		"format": "eslint . --fix --quiet",
		"lint": "eslint . --quiet",
		"build": "ncc build src/index.ts -o dist -m -s",
		"start": "node index.ts"
	},
	"dependencies": {
		"fastify": "^3.22.0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.38",
		"yup": "^0.32.11"
	}
}
